Cell[TextData[{
 "Escriba un m\[OAcute]dulo ",
 StyleBox["Mathematica",
  FontSlant->"Italic"],
 " que, teniendo como entrada una palabra x, y un entero k, devuelva el \
conjunto de segmentos de x de longitud k."
}], "ItemNumbered",
 CellChangeTimes->{
  3.587281372564268*^9, {3.58728148594456*^9, 3.587281550798246*^9}, {
   3.587283510258602*^9, 3.587283510357462*^9}, {3.784742385216391*^9, 
   3.7847423861204147`*^9}},ExpressionUUID->"f642cac1-c56e-4a83-a794-\
33df864d1285"],

Cell[TextData[StyleBox["Pista: La \[UAcute]ltima posici\[OAcute]n en la que \
puede empezar un segmento de longitud k es la posici\[OAcute]n n-k+1, siendo \
n la longitud de la palabra.",
 FontSize->11]], "Text",
 CellChangeTimes->{{3.587281606767194*^9, 3.587281675346916*^9}, {
  3.587281707489728*^9, 
  3.587281913367154*^9}},ExpressionUUID->"38210da3-2236-4c45-ba0b-\
87d290273db9"],

Cell[BoxData[
 RowBox[{
  RowBox[{"Segmentos", "[", 
   RowBox[{"x_", ",", " ", "k_"}], "]"}], " ", ":=", " ", 
  RowBox[{"Module", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{"segmentos", ",", " ", "i"}], "}"}], ",", "\[IndentingNewLine]", 
    
    RowBox[{
     RowBox[{"segmentos", " ", "=", " ", 
      RowBox[{"{", "}"}]}], ";", "\[IndentingNewLine]", "\[IndentingNewLine]", 
     RowBox[{"For", "[", 
      RowBox[{
       RowBox[{"i", " ", "=", " ", "1"}], ",", " ", 
       RowBox[{"i", " ", "\[LessEqual]", " ", 
        RowBox[{
         RowBox[{"Length", "[", "x", "]"}], "-", "k", "+", "1"}]}], ",", " ", 
       
       RowBox[{"i", "++"}], ",", "\[IndentingNewLine]", 
       RowBox[{
        RowBox[{"segmentos", " ", "=", " ", 
         RowBox[{"Append", "[", 
          RowBox[{"segmentos", ",", " ", 
           RowBox[{"Take", "[", 
            RowBox[{"x", ",", " ", 
             RowBox[{"{", 
              RowBox[{"i", ",", " ", 
               RowBox[{"i", "+", "k", "-", "1"}]}], "}"}]}], "]"}]}], "]"}]}],
         ";", "\[IndentingNewLine]", 
        RowBox[{"Return", "[", "segmentos", "]"}], ";"}]}], 
      "\[IndentingNewLine]", "]"}]}]}]}]}]], "Input",
