Cell["\<\
Implemente un m\[OAcute]dulo en Mathematica que, tomando como entrada una \
gram\[AAcute]tica incontextual devuelva True si est\[AAcute] en Forma Normal \
de Chomsky y False en caso contrario
\
\>", "Text",
 CellChangeTimes->{
  3.698555627465659*^9, {3.6985556634192543`*^9, 
   3.698555669450569*^9}},ExpressionUUID->"4a1e935f-280c-42e8-811a-\
4f125d4a2419"],

Cell[BoxData[
 RowBox[{
  RowBox[{
   RowBox[{"EsChomsky", "[", "grammar_", "]"}], ":=", 
   RowBox[{"Module", "[", 
    RowBox[{
     RowBox[{"{", 
      RowBox[{
      "sol", ",", "operations", ",", "auxiliar", ",", "terminal", ",", 
       "antecedent", ",", "consequent", ",", "n", ",", "k", ",", "p", ",", 
       "test"}], "}"}], ",", "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{"auxiliar", " ", "=", " ", 
       RowBox[{"grammar", "[", 
        RowBox[{"[", "1", "]"}], "]"}]}], ";", "\[IndentingNewLine]", 
      RowBox[{"terminal", " ", "=", " ", 
       RowBox[{"grammar", "[", 
        RowBox[{"[", "2", "]"}], "]"}]}], ";", "\[IndentingNewLine]", 
      RowBox[{"operations", " ", "=", " ", 
       RowBox[{"grammar", "[", 
        RowBox[{"[", "3", "]"}], "]"}]}], ";", "\[IndentingNewLine]", 
      RowBox[{"(*", 
       RowBox[{"Recorremos", " ", "todas", " ", "las", " ", "producciones"}], 
       "*)"}], "\[IndentingNewLine]", 
      RowBox[{"For", "[", 
       RowBox[{
        RowBox[{"n", "=", "1"}], ",", 
        RowBox[{"n", "\[LessEqual]", " ", 
         RowBox[{"Length", "[", "operations", "]"}]}], ",", 
        RowBox[{"n", "++"}], ",", "\[IndentingNewLine]", 
        RowBox[{
         RowBox[{"antecedent", " ", "=", " ", 
          RowBox[{
           RowBox[{"operations", "[", 
            RowBox[{"[", "n", "]"}], "]"}], "[", 
           RowBox[{"[", "1", "]"}], "]"}]}], ";", "\[IndentingNewLine]", 
         RowBox[{"consequent", " ", "=", " ", 
          RowBox[{
           RowBox[{"operations", "[", 
            RowBox[{"[", "n", "]"}], "]"}], "[", 
           RowBox[{"[", "2", "]"}], "]"}]}], ";", "\[IndentingNewLine]", 
         RowBox[{"(*", 
          RowBox[{
           RowBox[{
           "Comprobamos", " ", "que", " ", "sea", " ", "incontextual"}], ",", 
           " ", 
           RowBox[{"aunque", " ", "no", " ", "es", " ", "necesario"}]}], 
          "*)"}], "\[IndentingNewLine]", 
         RowBox[{"If", "[", 
          RowBox[{
           RowBox[{
            RowBox[{"Not", "[", 
             RowBox[{
              RowBox[{"Length", "[", "antecedent", "]"}], "\[Equal]", "1"}], 
             "]"}], "||", 
            RowBox[{"Not", "[", 
             RowBox[{"MemberQ", "[", 
              RowBox[{"auxiliar", ",", 
               RowBox[{"antecedent", "[", 
                RowBox[{"[", "1", "]"}], "]"}]}], "]"}], "]"}]}], ",", 
           "\[IndentingNewLine]", 
           RowBox[{
            RowBox[{"Return", "[", "False", "]"}], ";"}]}], 
          "\[IndentingNewLine]", "]"}], ";", "\[IndentingNewLine]", 
         RowBox[{"(*", 
          RowBox[{
          "Para", " ", "cada", " ", "antecedente", " ", "recorremos", " ", 
           "los", " ", "consecuentes"}], "*)"}], "\[IndentingNewLine]", 
         RowBox[{"For", "[", 
          RowBox[{
           RowBox[{"k", "=", "1"}], ",", 
           RowBox[{"k", "\[LessEqual]", " ", 
            RowBox[{"Length", "[", "consequent", "]"}]}], ",", 
           RowBox[{"k", "++"}], ",", "\[IndentingNewLine]", 
           RowBox[{
            RowBox[{"test", " ", "=", " ", 
             RowBox[{"consequent", "[", 
              RowBox[{"[", "k", "]"}], "]"}]}], ";", "\[IndentingNewLine]", 
            RowBox[{"(*", 
             RowBox[{
              RowBox[{
              "Si", " ", "el", " ", "consecuente", " ", "tiene", " ", "un", 
               " ", "solo", " ", "s\[IAcute]mbolo"}], ",", " ", 
              RowBox[{
              "\[EAcute]ste", " ", "tiene", " ", "que", " ", "ser", " ", 
               "terminal"}]}], "*)"}], "\[IndentingNewLine]", 
            RowBox[{"If", "[", 
             RowBox[{
              RowBox[{
               RowBox[{"Length", "[", "test", "]"}], "\[Equal]", "1"}], ",", 
              "\[IndentingNewLine]", 
              RowBox[{
               RowBox[{"If", "[", 
                RowBox[{
                 RowBox[{"Not", "[", 
                  RowBox[{"MemberQ", "[", 
                   RowBox[{"terminal", ",", 
                    RowBox[{"test", "[", 
                    RowBox[{"[", "1", "]"}], "]"}]}], "]"}], "]"}], ",", 
                 "\[IndentingNewLine]", 
                 RowBox[{
                  RowBox[{"Return", "[", "False", "]"}], ";"}]}], 
                "\[IndentingNewLine]", "]"}], ";"}], "\[IndentingNewLine]", 
              ",", "\[IndentingNewLine]", 
              RowBox[{"(*", 
               RowBox[{
                RowBox[{
                "Si", " ", "el", " ", "consecuente", " ", "tiene", " ", "dos",
                  " ", "s\[IAcute]mbolos"}], ",", " ", 
                RowBox[{
                "ambos", " ", "tienen", " ", "que", " ", "ser", " ", 
                 "auxiliares"}]}], "*)"}], "\[IndentingNewLine]", 
              RowBox[{
               RowBox[{"If", "[", 
                RowBox[{
                 RowBox[{
                  RowBox[{"Length", "[", "test", "]"}], "\[Equal]", "2"}], 
                 ",", "\[IndentingNewLine]", 
                 RowBox[{
                  RowBox[{"If", "[", 
                   RowBox[{
                    RowBox[{
                    RowBox[{"Not", "[", 
                    RowBox[{"MemberQ", "[", 
                    RowBox[{"auxiliar", ",", 
                    RowBox[{"test", "[", 
                    RowBox[{"[", "1", "]"}], "]"}]}], "]"}], "]"}], "||", 
                    RowBox[{"Not", "[", 
                    RowBox[{"MemberQ", "[", 
                    RowBox[{"auxiliar", ",", 
                    RowBox[{"test", "[", 
                    RowBox[{"[", "2", "]"}], "]"}]}], "]"}], "]"}]}], ",", 
                    "\[IndentingNewLine]", 
                    RowBox[{
                    RowBox[{"Return", "[", "False", "]"}], ";"}]}], 
                   "\[IndentingNewLine]", "]"}], ";"}], "\[IndentingNewLine]",
                  ",", "\[IndentingNewLine]", 
                 RowBox[{"(*", 
                  RowBox[{
                   RowBox[{
                   "Si", " ", "no", " ", "se", " ", "cumple", " ", "ninguna", 
                    " ", "de", " ", "las", " ", "dos", " ", "condiciones", 
                    " ", "anteriores"}], ",", " ", 
                   RowBox[{
                   "no", " ", "est\[AAcute]", " ", "en", " ", "Forma", " ", 
                    "Normal", " ", "de", " ", "Chomsky"}]}], "*)"}], 
                 "\[IndentingNewLine]", 
                 RowBox[{
                  RowBox[{"Return", "[", "False", "]"}], ";"}]}], 
                "\[IndentingNewLine]", "]"}], ";"}]}], "\[IndentingNewLine]", 
             "]"}], ";"}]}], "\[IndentingNewLine]", "]"}], ";"}]}], 
       "\[IndentingNewLine]", "]"}], ";", "\[IndentingNewLine]", 
      RowBox[{"Return", " ", "[", "True", "]"}], ";"}]}], 
    "\[IndentingNewLine]", "]"}]}], ";"}]], "Input",
 CellChangeTimes->{{3.696701667064289*^9, 3.696701683585949*^9}, {
   3.6967017232032104`*^9, 3.6967018814149323`*^9}, {3.696701951151579*^9, 
