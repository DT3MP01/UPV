Cell[BoxData[{
 RowBox[{
  RowBox[{
   RowBox[{"IntToRegla", "[", "num_Integer", "]"}], ":=", 
   RowBox[{"Module", "[", 
    RowBox[{
     RowBox[{"{", 
      RowBox[{"res", ",", " ", "i", ",", "n"}], "}"}], ",", 
     "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{"res", " ", "=", " ", 
       RowBox[{"{", "}"}]}], ";", "\[IndentingNewLine]", 
      RowBox[{"i", "=", "0"}], ";", "\[IndentingNewLine]", 
      RowBox[{"n", "=", "num"}], ";", "\[IndentingNewLine]", 
      RowBox[{"While", "[", 
       RowBox[{
        RowBox[{"i", "<", "8"}], ",", "\[IndentingNewLine]", 
        RowBox[{
         RowBox[{"AppendTo", "[", 
          RowBox[{"res", ",", 
           RowBox[{"{", 
            RowBox[{
             RowBox[{"Mod", "[", 
              RowBox[{
               RowBox[{"Quotient", "[", 
                RowBox[{"i", ",", "4"}], "]"}], ",", "2"}], "]"}], ",", 
             RowBox[{"Mod", "[", 
              RowBox[{
               RowBox[{"Quotient", "[", 
                RowBox[{"i", ",", "2"}], "]"}], ",", "2"}], "]"}], ",", 
             RowBox[{"Mod", "[", 
              RowBox[{"i", ",", "2"}], "]"}], ",", 
             RowBox[{"Mod", "[", 
              RowBox[{"n", ",", "2"}], "]"}]}], "}"}]}], "]"}], ";", 
         "\[IndentingNewLine]", 
         RowBox[{"n", "=", 
          RowBox[{"Quotient", "[", 
           RowBox[{"n", ",", "2"}], "]"}]}], ";", "\[IndentingNewLine]", 
         RowBox[{"i", "++"}], ";"}]}], "\[IndentingNewLine]", "]"}], ";", 
      "\[IndentingNewLine]", 
      RowBox[{"Return", "[", "res", "]"}], ";"}]}], "\[IndentingNewLine]", 
    "]"}]}], ";"}], "\n", 
 RowBox[{
  RowBox[{
   RowBox[{"ACItera", "[", 
    RowBox[{"inicial_List", ",", "regla_List"}], "]"}], ":=", 
   RowBox[{"Module", "[", 
    RowBox[{
     RowBox[{"{", 
      RowBox[{"final", ",", "i"}], "}"}], ",", "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{"final", "=", 
       RowBox[{"{", "}"}]}], ";", "\[IndentingNewLine]", 
      RowBox[{"For", "[", 
       RowBox[{
        RowBox[{"i", "=", "1"}], ",", 
        RowBox[{"i", "\[LessEqual]", 
         RowBox[{"Length", "[", "inicial", "]"}]}], ",", 
        RowBox[{"i", "++"}], ",", "\[IndentingNewLine]", 
        RowBox[{
         RowBox[{"AppendTo", "[", 
          RowBox[{"final", ",", 
           RowBox[{
            RowBox[{"Cases", "[", 
             RowBox[{"regla", ",", 
              RowBox[{"{", 
               RowBox[{
                RowBox[{"inicial", "[", 
                 RowBox[{"[", 
                  RowBox[{
                   RowBox[{"Mod", "[", 
                    RowBox[{
                    RowBox[{"i", "-", "2"}], ",", 
                    RowBox[{"Length", "[", "inicial", "]"}]}], "]"}], "+", 
                   "1"}], "]"}], "]"}], ",", 
                RowBox[{"inicial", "[", 
                 RowBox[{"[", "i", "]"}], "]"}], ",", 
                RowBox[{"inicial", "[", 
                 RowBox[{"[", 
                  RowBox[{
                   RowBox[{"Mod", "[", 
                    RowBox[{"i", ",", 
                    RowBox[{"Length", "[", "inicial", "]"}]}], "]"}], "+", 
                   "1"}], "]"}], "]"}], ",", "_"}], "}"}]}], "]"}], "[", 
            RowBox[{"[", 
             RowBox[{"1", ",", "4"}], "]"}], "]"}]}], "]"}], ";"}]}], 
       "\[IndentingNewLine]", "]"}], ";", "\[IndentingNewLine]", 
