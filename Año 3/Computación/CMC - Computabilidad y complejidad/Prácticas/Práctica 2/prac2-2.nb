Cell[BoxData[
 RowBox[{
  RowBox[{
   RowBox[{"ACToDiagram", "[", 
    RowBox[{"string_", ",", "AC_", ",", "q_"}], "]"}], ":=", 
   RowBox[{"Module", "[", 
    RowBox[{
     RowBox[{"{", 
      RowBox[{
      "res", ",", "i", ",", "j", ",", "aux", ",", "representation", ",", " ", 
       "assignedColours", ",", " ", "colourAssignment", ",", " ", "rules"}], 
      "}"}], ",", "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{"res", "=", "string"}], ";", "\[IndentingNewLine]", 
      RowBox[{"representation", " ", "=", " ", 
       RowBox[{"{", "string", "}"}]}], ";", "\[IndentingNewLine]", 
      RowBox[{"rules", " ", "=", " ", 
       RowBox[{"AC", "[", 
        RowBox[{"[", "2", "]"}], "]"}]}], ";", "\[IndentingNewLine]", 
      RowBox[{"For", "[", 
       RowBox[{
        RowBox[{"i", "=", "0"}], ",", 
        RowBox[{"i", "<", 
         RowBox[{"Length", "[", "res", "]"}]}], ",", 
        RowBox[{"++", "i"}], ",", "\[IndentingNewLine]", 
        RowBox[{"(*", " ", 
         RowBox[{"aux", " ", "=", " ", 
          RowBox[{
           RowBox[{"s", " ", "\[Rule]", " ", 
            RowBox[{"Searching", " ", 
             RowBox[{"for", ":", " ", "s"}]}]}], " ", "=", " ", 
           RowBox[{"f", 
            RowBox[{"(", 
             RowBox[{"q", ",", " ", 
              RowBox[{"string", "[", "1", "]"}]}], " "}]}]}]}], "*)"}], 
        "\[IndentingNewLine]", 
        RowBox[{
         RowBox[{"aux", "=", 
          RowBox[{"{", 
           RowBox[{
            RowBox[{"Cases", "[", 
             RowBox[{"rules", ",", 
              RowBox[{"{", 
               RowBox[{"q", ",", 
                RowBox[{"res", "[", 
                 RowBox[{"[", "1", "]"}], "]"}], ",", "_"}], "}"}]}], "]"}], 
            "[", 
            RowBox[{"[", 
             RowBox[{"1", ",", "3"}], "]"}], "]"}], "}"}]}], ";", 
         "\[IndentingNewLine]", 
         RowBox[{"For", "[", 
          RowBox[{
           RowBox[{"j", "=", "1"}], ",", 
           RowBox[{"j", " ", "<", 
            RowBox[{"Length", "[", "res", "]"}]}], ",", 
           RowBox[{"++", "j"}], ",", "\[IndentingNewLine]", 
           RowBox[{"(*", " ", 
            RowBox[{
            "Appending", " ", "to", " ", "aux", " ", "the", " ", "state", " ",
              "result", " ", "of", " ", "the", " ", "transition"}], " ", 
            "*)"}], "\[IndentingNewLine]", 
           RowBox[{
            RowBox[{"AppendTo", "[", 
             RowBox[{"aux", ",", 
              RowBox[{
               RowBox[{"Cases", "[", 
                RowBox[{"rules", ",", 
                 RowBox[{"{", 
                  RowBox[{
                   RowBox[{"res", "[", 
                    RowBox[{"[", "j", "]"}], "]"}], ",", 
                   RowBox[{"res", "[", 
                    RowBox[{"[", 
                    RowBox[{"j", "+", "1"}], "]"}], "]"}], ",", "_"}], 
                  "}"}]}], "]"}], "[", 
               RowBox[{"[", 
                RowBox[{"1", ",", "3"}], "]"}], "]"}]}], "]"}], ";"}]}], 
          "\[IndentingNewLine]", "]"}], ";", "\[IndentingNewLine]", 
         RowBox[{"PrependTo", "[", 
          RowBox[{"representation", ",", "aux"}], "]"}], ";", 
         "\[IndentingNewLine]", 
         RowBox[{"res", "=", "aux"}], ";"}]}], "\[IndentingNewLine]", "]"}], 
      ";", "\[IndentingNewLine]", "\[IndentingNewLine]", 
      RowBox[{"(*", " ", 
       RowBox[{
       "Assigning", " ", "a", " ", "colour", " ", "to", " ", "each", " ", 
        "state"}], " ", "*)"}], "\[IndentingNewLine]", 
      RowBox[{"assignedColours", " ", "=", 
       RowBox[{"RandomColor", "[", 
        RowBox[{"Length", "[", 
         RowBox[{"AC", "[", 
          RowBox[{"[", "1", "]"}], "]"}], "]"}], "]"}]}], ";", 
      "\[IndentingNewLine]", 
      RowBox[{"colourAssignment", "=", 
       RowBox[{"{", "}"}]}], ";", "\[IndentingNewLine]", 
      RowBox[{"For", "[", 
       RowBox[{
        RowBox[{"i", "=", "1"}], ",", 
        RowBox[{"i", "\[LessEqual]", "  ", 
